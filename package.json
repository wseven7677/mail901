{
  "name": "mail901",
  "version": "0.0.1",
  "description": "node email app",
  "main": "index.js",
  "scripts": {
    "start": "npm run boot && babel-node app/index.js",
    "test": "babel-node test/test.js",
    "debug": "npm run boot && babel-node --inspect app/index.js",
    "boot": "babel-node app/lib/boot.js",
    "bapp": "rm -rf dist/app && npm run boot && babel app -d dist/app",
    "bscript": "rm -rf dist/script && babel script -d dist/script",
    "bconfig": "rm -rf dist/config && babel config -d dist/config",
    "build": "npm run bapp && npm run bscript && npm run bconfig"
  },
  "keywords": [
    "node",
    "email"
  ],
  "author": "901",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "moment": "^2.24.0",
    "multer": "^1.4.2",
    "node-schedule": "^1.3.2",
    "nodemailer": "^6.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0"
  }
}
